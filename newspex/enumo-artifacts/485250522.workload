(plug "bop" {+ - *}
  (plug "uop" {~}
    (plug "var" {a b c}
      (plug "lit" {-1 0 1}
        (filter (contains var)
          (filter (#lists < 3)
            (plug "lang"
              (plug "lang" {lit var}
                {lit var (uop lang) (bop lang lang)})
              {lit var (uop lang) (bop lang lang)})))))))
