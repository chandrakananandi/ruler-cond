(plug "bop" {& | ^ ->}
  (plug "uop" {~}
    (plug "var" {ba bb bc}
      (plug "lit" {true false}
        (filter (contains var)
          (filter (#lists < 3)
            (plug "lang"
              (plug "lang" {lit var}
                {lit var (uop lang) (bop lang lang)})
              {lit var (uop lang) (bop lang lang)})))))))
